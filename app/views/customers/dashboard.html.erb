<p id="notice"><%= notice %></p>

<h1>Bookings</h1>

<table>
  <thead>
    <tr>
      <th>From date</th>
      <th>To date</th>
      <th>Customer</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bookings.each do |booking| %>
      <tr>
        <td><%= booking.from_date %></td>
        <td><%= booking.to_date %></td>
        <td><%= booking.customer_id %></td>
        <td><%= booking.status %></td>
        <td><%= link_to 'Show', booking %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

<br>

